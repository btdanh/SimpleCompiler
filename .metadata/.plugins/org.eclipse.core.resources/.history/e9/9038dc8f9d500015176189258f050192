grammar Decaf;

/* Parser grammar */
program: CLASS PROGRAM BRACKET_OPEN field_decl_s method_decl_s BRACKET_CLOSE;

field_decl_s: (field_decl)*;
field_decl: ;

method_decl_s: (method_decl)*;
method_decl:;


/*Lexer grammar */
CLASS: 'class';
PROGRAM: 'Program';
BRACKET_OPEN: '{';
BRACKET_CLOSE: '}';

